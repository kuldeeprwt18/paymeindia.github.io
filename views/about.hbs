  <script>



    const signupPop = document.getElementsByClassName("SignUpModal")[0];

    window.addEventListener("load", function () {

      showPopup();

    })

    function showPopup() {
      const timeLimit = 5 // seconds;
      let i = 0;
      const timer = setInterval(function () {
        i++;
        if (i == timeLimit) {
          clearInterval(timer);
          signupPop.style.display = "block";
